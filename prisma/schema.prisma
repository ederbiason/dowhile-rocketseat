generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// Tabela usuaria
model User {
  id          String @id @default(uuid())
  name        String
  github_id   Int 
  avatar_url  String
  login       String

  // Define que a tabela vai ter nome de "users"
  @@map("users")
}
